<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.owl.spring.member.domain.Member" alias="Member"/>
		<typeAlias type="com.owl.spring.notice.domain.Notice" alias="Notice"/>
		<typeAlias type="com.owl.spring.board.domain.Board" alias="Board"/>
		<typeAlias type="com.owl.spring.board.domain.Reply" alias="Reply"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/member/member-mapper.xml"/>
		<mapper resource="mappers/notice/notice-mapper.xml"/>
		<mapper resource="mappers/board/board-mapper.xml"/>
		<mapper resource="mappers/reply/reply-mapper.xml"/>
	</mappers>
</configuration>